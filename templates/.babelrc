{
  "env": {
    "development": {
      "presets": [
        "react",
        "es2015-ie",
        "stage-1",
        "react-hmre"
      ],
      "plugins": [
        "mobx-deep-action",
        "transform-decorators-legacy", ["import", {
          "libraryName": "antd",
          "style": true
        }],
        "transform-runtime"
      ]
    },
    "production": {
      "presets": [
        "react",
        "es2015-ie",
        "stage-1"
      ],
      "plugins": [
        "mobx-deep-action",
        "transform-decorators-legacy", ["import", {
          "libraryName": "antd",
          "style": true
        }],
        "transform-runtime"
      ]
    }
  }
}
